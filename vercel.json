{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "crons": [
    {
      "path": "/api/",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/updateData/lotoFacil",
      "schedule": "0 22 * * *"
    },
    {
      "path": "/api/updateData/megaSena",
      "schedule": "30 22 * * 2,4,6"
    },
    {
      "path": "/api/updateData/quina",
      "schedule": "0 23 * * 1-6"
    }
  ]
}
